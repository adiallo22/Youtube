    <tags>sqlite pcl database monotouch ios monodroid android wp8 wpa</tags>
    <dependencies>
      <group targetFramework="Xamarin.iOS1.0">
        <dependency id="SQLitePCLRaw.core" version="1.0.0" />
      </group>
    </dependencies>
  </metadata>
</package>